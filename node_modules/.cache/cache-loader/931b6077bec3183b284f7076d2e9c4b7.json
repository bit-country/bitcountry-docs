yB,KAAzB,CALM,CAKmC;AAC3C;AACA,kBAPQ,CADF,CASE,SAAG,UAAU,CAAC,IAAd,EAAmB,WAAK,GAAG,CAAE,yBAAV,CAAqC,GAAG,CAAED,OAAO,CAAC,yHAAD,CAAP,CAAmIC,OAA7K,EAAnB,CATF,CApBF,CA+BE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB,qDAArB,CAA0E,kBAAY,UAAU,CAAC,GAAvB,EAA6B,kBAA7B,CAA1E,CAAwI,6HAAxI,CAAqQ,kBAAY,UAAU,CAAC,GAAvB,EAA6B,kBAA7B,CAArQ,CAAmU,IAAnU,CADF,CA/BF,CAtEK,CAyGL,aAAK,IAAL,CAAS,WAAK,GAAG,CAAE,kBAAV,CAA8B,GAAG,CAAED,OAAO,CAAC,+HAAD,CAAP,CAAyIC,OAA5K,EAAT,CAzGK,CA0GL,cACE,UAAI,UAAU,CAAC,IAAf,EACE,SAAG,UAAU,CAAC,IAAd,EAAqB;AAC7B,iBADQ,CADF,CAGE,SAAG,UAAU,CAAC,IAAd,EAAmB,WAAK,GAAG,CAAE,0BAAV,CAAsC,GAAG,CAAED,OAAO,CAAC,+HAAD,CAAP,CAAyIC,OAApL,EAAnB,CAHF,CAIE,SAAG,UAAU,CAAC,IAAd,EAAmB,WAAK,GAAG,CAAE,SAAV,CAAqB,GAAG,CAAED,OAAO,CAAC,iHAAD,CAAP,CAA2HC,OAArJ,EAAnB,CAJF,CADF,CA1GK,CAAP,CAmHD,CAED,CACAJ,UAAU,CAACK,cAAX,CAA4B,IAA5B","sourcesContent":["\n  import React from 'react';\n  import { mdx } from '@mdx-js/react';\n\n  export const frontMatter = {\n\tid: 'how-to-participate-in-derby',\n\ttitle: 'How To Participate in Zeitgeist\\'s Kusama Parachain Derby'\n};\nexport const metadata = {\n  \"unversionedId\": \"how-to-participate-in-derby\",\n  \"id\": \"how-to-participate-in-derby\",\n  \"isDocsHomePage\": false,\n  \"title\": \"How To Participate in Zeitgeist's Kusama Parachain Derby\",\n  \"description\": \"Create a Zeitgeist account\",\n  \"source\": \"@site/docs/how-to-participate-in-derby.md\",\n  \"slug\": \"/how-to-participate-in-derby\",\n  \"permalink\": \"/how-to-participate-in-derby\",\n  \"editUrl\": \"https://github.com/zeitgeistpm/docs/edit/main/docs/how-to-participate-in-derby.md\",\n  \"version\": \"current\",\n  \"sidebar\": \"someSidebar\",\n  \"previous\": {\n    \"title\": \"Roadmap\",\n    \"permalink\": \"/roadmap\"\n  },\n  \"next\": {\n    \"title\": \"Using the Faucet\",\n    \"permalink\": \"/using-the-faucet\"\n  }\n};\n  /* @jsxRuntime classic */\n/* @jsx mdx */\n\nexport const rightToc = [{\n  value: 'Create a Zeitgeist account',\n  id: 'create-a-zeitgeist-account',\n  children: [{\n    value: 'PC',\n    id: 'pc',\n    children: []\n  }]\n}, {\n  value: 'Ask Faucet for Testnet Tokens (ZBP)',\n  id: 'ask-faucet-for-testnet-tokens-zbp',\n  children: []\n}, {\n  value: 'Make your Prediction on Zeitgeist',\n  id: 'make-your-prediction-on-zeitgeist',\n  children: []\n}];\n\nconst layoutProps = {\n  rightToc\n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n    <h2 {...{\n      \"id\": \"create-a-zeitgeist-account\"\n    }}>{`Create a Zeitgeist account`}</h2>\n    <h3 {...{\n      \"id\": \"pc\"\n    }}>{`PC`}</h3>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`The easiet way to create and maintain accounts in the polkadot ecosystem is\nusing the `}<inlineCode parentName=\"p\">{`Polkadot{.js} extension`}</inlineCode>{` for Chrome or Firefox, both available\n`}<a parentName=\"p\" {...{\n            \"href\": \"https://polkadot.js.org/extension/\"\n          }}>{`here`}</a></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`You will need an account, which you can create using the extension.\n`}<strong parentName=\"p\">{`AttentionÔºÅ`}</strong>{` You `}<strong parentName=\"p\">{`MUST`}</strong>{` write down and secure your mnemonic seed which\ncan be used to restore your wallet. Keep it secure as it will recreate your\naccount without a password or any further authorisation.`}</p>\n        <p parentName=\"li\"><img alt={\"create a new account\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506183818.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`When finished, you will get an account like this.`}</p>\n        <p parentName=\"li\"><img alt={\"finish account creation\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506184035.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Select either \"Use on any chain\" or \"Zeitgeist Battery Park\" in the\nside menu of the new account`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Copy your new Zeitgeist address, you can use it now!`}</p>\n      </li>\n    </ul>\n    <p>{`(If you already loaded the Kusama Derby page, you may need to reload before it\nconnects to the extension)`}</p>\n    <h2 {...{\n      \"id\": \"ask-faucet-for-testnet-tokens-zbp\"\n    }}>{`Ask Faucet for Testnet Tokens (ZBP)`}</h2>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`To participate in the Derby, you'll first need to get our testnet token - test\nZTG.`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Join our `}<a parentName=\"p\" {...{\n            \"href\": \"https://discord.com/invite/xv8HuA4s8v\"\n          }}>{`Discord`}</a>{` and enter the room\nnamed faucet.`}</p>\n        <p parentName=\"li\"><img alt={\"faucet screenshot\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506185713.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Send a message like this `}<inlineCode parentName=\"p\">{`!drip 5Hxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx`}</inlineCode>{`\nto get test ZTG sent to your account. In this command, replace\n`}<inlineCode parentName=\"p\">{`5Hxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx`}</inlineCode>{` with your Zeitgeist address.\nMake sure this is the address you have access to - you won't get a second\nchance!`}</p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`If you had a KSM account with a KSM balance on May 11, 2021 when the snapshot was taken,\nyou can also claim an equivalent amount of ZBP from the faucet. See `}<a parentName=\"p\" {...{\n            \"href\": \"/using-the-faucet\"\n          }}>{`using the faucet`}</a>{`\nfor instructions on how that can be done.`}</p>\n      </li>\n    </ul>\n    <h2 {...{\n      \"id\": \"make-your-prediction-on-zeitgeist\"\n    }}>{`Make your Prediction on Zeitgeist`}</h2>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Open up `}<a parentName=\"p\" {...{\n            \"href\": \"https://proto.zeitgeist.pm/kusama-derby\"\n          }}>{`the Kusama Derby dapp`}</a></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`First, you need to link your wallet to our dapp. Just click this.`}</p>\n        <p parentName=\"li\"><img alt={\"link wallet to dapp\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506190512.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`Now you can make a prediction!`}</p>\n        <p parentName=\"li\"><img alt={\"enter the prediction market\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/derby_markets.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`After choosing which slot you want to predict, you'll see the current state of that market - the further the 'horse', the more ZBP was bet on it. Below, you can add whichever prediction you choose.`}</p>\n        <p parentName=\"li\"><img alt={\"choose a slot\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506190815.png\").default} /></p>\n        <p parentName=\"li\"><img alt={\"prediction situation\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/derby_horses.png\").default} /></p>\n        <p parentName=\"li\"><img alt={\"buy &amp; sell\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506191144.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`If you want to bet on a horse winning, you want to `}<strong parentName=\"p\">{`BUY`}</strong>{` - just click the\n`}<inlineCode parentName=\"p\">{`BUY`}</inlineCode>{` button and fill in the amount. The top number is how many ZTG you want\nto spend on your bet, and the lower one will update with how many `}<strong parentName=\"p\">{`OUTCOME\nTOKENS`}</strong>{` you will receive. For example, if you want to bet 10 ZTG, because the\n`}<inlineCode parentName=\"p\">{`Price`}</inlineCode>{` is 0.1422, you can get 10/0.1422=70.32 OUTCOME TOKENS (excluding the\n`}<strong parentName=\"p\">{`Fee`}</strong>{`) which can be converted to 70.32 ZTG (minus the fee) if your horse\ncomes in first. Similarly, if you want to receive 10 ZTG if you win, the cost\nis 1.422 ZTG now.`}</p>\n        <p parentName=\"li\"><img alt={\"image-20210506191332151\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/derby_open_card.png\").default} /></p>\n      </li>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`When you've entered the details of your bet, click `}<inlineCode parentName=\"p\">{`Sign Transaction`}</inlineCode>{` and you will get this page. You will need to confirm using the password you set up for your Zeitgeist account, and click  `}<inlineCode parentName=\"p\">{`Sign Transaction`}</inlineCode>{` .`}</p>\n      </li>\n    </ul>\n    <p>{`  `}<img alt={\"Sign transaction\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506192656.png\").default} /></p>\n    <ul>\n      <li parentName=\"ul\">\n        <p parentName=\"li\">{`If you can see the message 'Swapped!', you placed your bet successfully.\nCongratulations!`}</p>\n        <p parentName=\"li\"><img alt={\"Broadcasting transaction\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/how-to/20210506192649.png\").default} /></p>\n        <p parentName=\"li\"><img alt={\"success\"} src={require(\"!url-loader?limit=10000&name=assets/images/[name]-[hash].[ext]&fallback=file-loader!./../static/img/success.png\").default} /></p>\n      </li>\n    </ul>\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}